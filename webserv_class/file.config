server {
    listen 127.0.0.1 7000

    server_name test.com www.test.com

    error_page 500 ./error_pages/500.html
    error_page 404 ./error_pages/403.html

    max_body_size 1

    location /test [
        allowed_methods POST DELETE GET
        
        cgi py /usr/bin/python
        cgi php ./bin/php-cgi
        autoindex off
    ]
}